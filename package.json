{
  "name": "vschn",
  "displayName": "vschn",
  "description": "YCombinator HackerNews reader for VSCode.",
  "version": "0.0.12",
  "publisher": "Kyter",
  "repository": {
    "type": "git",
    "url": "https://github.com/Kyter-com/vschn"
  },
  "icon": "public/icon.png",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vschn.start",
        "title": "vschn: Start"
      }
    ],
    "configuration": {
      "title": "vschn",
      "properties": {
        "vschn.defaultLanguage": {
          "type": "string",
          "enum": [
            "HTML",
            "Markdown",
            "TypeScript"
          ],
          "description": "Select the default language to skip the question on each startup."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/node": "20.11.30",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint": "^8.55.0",
    "glob": "^10.3.10",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.6.2"
  }
}
